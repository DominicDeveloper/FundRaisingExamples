<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/splashFragment">
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.projectworkaprilkumak.onboarding.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_intro1Fragment"
            app:destination="@id/intro1Fragment" />
        <action
            android:id="@+id/action_splashFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
    </fragment>
    <fragment
        android:id="@+id/intro1Fragment"
        android:name="com.example.projectworkaprilkumak.onboarding.Intro1Fragment"
        android:label="fragment_intro1"
        tools:layout="@layout/fragment_intro1" >
        <action
            android:id="@+id/action_intro1Fragment_to_intro2Fragment"
            app:destination="@id/intro2Fragment" />
        <action
            android:id="@+id/action_intro1Fragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/intro2Fragment"
        android:name="com.example.projectworkaprilkumak.onboarding.Intro2Fragment"
        android:label="fragment_intro2"
        tools:layout="@layout/fragment_intro2" >
        <action
            android:id="@+id/action_intro2Fragment_to_intro3Fragment"
            app:destination="@id/intro3Fragment" />
        <action
            android:id="@+id/action_intro2Fragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/intro3Fragment"
        android:name="com.example.projectworkaprilkumak.onboarding.Intro3Fragment"
        android:label="fragment_intro3"
        tools:layout="@layout/fragment_intro3" >
        <action
            android:id="@+id/action_intro3Fragment_to_signInFragment"
            app:destination="@id/signInFragment" />
        <action
            android:id="@+id/action_intro3Fragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.example.projectworkaprilkumak.onboarding.WelcomeFragment"
        android:label="fragment_welcome"
        tools:layout="@layout/fragment_welcome" >
        <action
            android:id="@+id/action_welcomeFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
        <action
            android:id="@+id/action_welcomeFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.example.projectworkaprilkumak.onboarding.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <action
            android:id="@+id/action_signInFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
        <action
            android:id="@+id/action_signInFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.example.projectworkaprilkumak.onboarding.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >
        <action
            android:id="@+id/action_signUpFragment_to_selectCountryFragment"
            app:destination="@id/selectCountryFragment" />
        <action
            android:id="@+id/action_signUpFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
    </fragment>
    <fragment
        android:id="@+id/selectCountryFragment"
        android:name="com.example.projectworkaprilkumak.onboarding.accaunt_setup.SelectCountryFragment"
        android:label="fragment_select_country"
        tools:layout="@layout/fragment_select_country" >
        <action
            android:id="@+id/action_selectCountryFragment_to_fillProfile"
            app:destination="@id/fillProfile" />
        <action
            android:id="@+id/action_selectCountryFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.projectworkaprilkumak.Home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_bookmarkFragment"
            app:destination="@id/bookmarkFragment" />
        <action
            android:id="@+id/action_homeFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
    </fragment>
    <fragment
        android:id="@+id/fillProfile"
        android:name="com.example.projectworkaprilkumak.onboarding.accaunt_setup.FillProfile"
        android:label="fragment_fill_profile"
        tools:layout="@layout/fragment_fill_profile" >
        <action
            android:id="@+id/action_fillProfile_to_selectInterestFragment"
            app:destination="@id/selectInterestFragment" />
        <action
            android:id="@+id/action_fillProfile_to_selectCountryFragment"
            app:destination="@id/selectCountryFragment" />
    </fragment>
    <fragment
        android:id="@+id/selectInterestFragment"
        android:name="com.example.projectworkaprilkumak.onboarding.accaunt_setup.SelectInterestFragment"
        android:label="fragment_select_interest"
        tools:layout="@layout/fragment_select_interest" >
        <action
            android:id="@+id/action_selectInterestFragment_to_createPinFragment"
            app:destination="@id/createPinFragment" />
        <action
            android:id="@+id/action_selectInterestFragment_to_fillProfile"
            app:destination="@id/fillProfile" />
    </fragment>
    <fragment
        android:id="@+id/createPinFragment"
        android:name="com.example.projectworkaprilkumak.onboarding.accaunt_setup.CreatePinFragment"
        android:label="fragment_create_pin"
        tools:layout="@layout/fragment_create_pin" >
        <action
            android:id="@+id/action_createPinFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_createPinFragment_to_selectInterestFragment"
            app:destination="@id/selectInterestFragment" />
    </fragment>
    <fragment
        android:id="@+id/bookmarkFragment"
        android:name="com.example.projectworkaprilkumak.Home.BookmarkFragment"
        android:label="fragment_bookmark"
        tools:layout="@layout/fragment_bookmark" >
        <action
            android:id="@+id/action_bookmarkFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.projectworkaprilkumak.Home.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>